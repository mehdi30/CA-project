                                    <i class="ace-icon fa fa-lock"></i>

                                </span>
                            </label>
    </div>
                                <div class="space"></div>
    <div  class="clearfix">     
        <input type="submit" class="pull-right btn btn-sm btn-primary" value="{{ 'Enregistrer'|trans({}, 'FOSUserBundle') }}" />
    </div>
  
    {{ form_rest(form) }}
    {{ form_end(form) }}
  